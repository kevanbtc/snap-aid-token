    {
      "label": "Frontend: build",
      "type": "shell",
      "command": "npm",
      "args": ["run", "build", "--prefix", "C:/snap-aid-token/web"]
    }
  ]
}{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run backend dev server",
			"type": "shell",
			"command": "powershell",
			"args": [
				"-NoProfile",
				"-Command",
				"cd C:\\snap-aid-token\\server; npm run dev"
