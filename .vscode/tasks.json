{
  "$schema": "https://json.schemastore.org/vscode/tasks.json",
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Backend: dev server",
      "type": "shell",
      "command": "npm",
      "args": ["run", "dev", "--prefix", "C:/snap-aid-token/server"],
      "isBackground": true,
      "problemMatcher": []
    },
    {
      "label": "Frontend: dev server",
      "type": "shell",
      "command": "npm",
      "args": ["run", "dev", "--prefix", "C:/snap-aid-token/web"],
      "isBackground": true,
      "problemMatcher": []
